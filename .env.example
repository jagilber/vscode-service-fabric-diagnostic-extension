# Example Environment Configuration
# Copy this file to .env and replace with your actual values
# 
# ⚠️ NEVER commit .env file - it contains real credentials!

# Test cluster endpoint (replace with your cluster)
SF_TEST_CLUSTER=https://mycluster.eastus.cloudapp.azure.com:19080

# Certificate thumbprint (replace with your certificate)
# Find with: Get-ChildItem Cert:\CurrentUser\My
SF_TEST_THUMBPRINT=1234567890ABCDEF1234567890ABCDEF12345678

# Local cluster (for development)
SF_LOCAL_CLUSTER=http://localhost:19080

# Enable integration tests (set to false for unit tests only)
RUN_INTEGRATION_TESTS=false
